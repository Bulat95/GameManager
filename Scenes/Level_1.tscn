func _ready():
	var style:StyleBoxFlat = StyleBoxFlat.new()
	style.bg_color = Color(100, 100, 100, 0.1)
	add_theme_stylebox_override(\"panel\", style)

func _physics_process(delta):
	if len(GlobalConst.products) == 0:
		return
	
	var product: Product = GlobalConst.products[0]
	if product.isCompleted():
		get_theme_stylebox(\"panel\").bg_color = Color(0, 100, 0, 0.5)
	else: 
		get_theme_stylebox(\"panel\").bg_color = Color(100, 100, 0, 0.5)
"

